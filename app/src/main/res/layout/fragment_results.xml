<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   tools:context=".ui.qcm.score.ScoreFragment"

                                                   android:layout_height="match_parent">
	<TextView
			android:text="Nombre de réponses bonnes"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/NbRepOk"
			app:layout_constraintTop_toTopOf="parent"
			android:layout_marginTop="145dp" app:layout_constraintStart_toStartOf="parent"
			android:layout_marginStart="16dp"/>

	<TextView
		android:id="@+id/NbRepFaux"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="16dp"
		android:layout_marginTop="64dp"
		android:text="Nombre de réponses fausses"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/TVRepOk" />

	<TextView
		android:id="@+id/NbRepVides"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="16dp"
		android:layout_marginTop="64dp"
		android:text="Nombre de questions non répondues"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/TVRepFaux" />

	<TextView
		android:id="@+id/TVRepOk"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="16dp"
		android:layout_marginTop="28dp"
		android:text="TextView"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/NbRepOk" />

	<TextView
		android:id="@+id/TVRepFaux"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="16dp"
		android:layout_marginTop="28dp"
		android:text="TextView"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/NbRepFaux" />

	<TextView
		android:id="@+id/TVRepVides"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="16dp"
		android:layout_marginTop="28dp"
		android:text="TextView"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/NbRepVides" />


</androidx.constraintlayout.widget.ConstraintLayout>


